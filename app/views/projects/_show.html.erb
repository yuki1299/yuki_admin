<div class="form-box tasks-box">
  <div class="form-title">
    <h2>
      Detalhes do teste
    </h2>
  </div> 
  <div class="template-example">
    <div class="title">
      <h3>Nome</h3>
    </div>
    <div class="template-content">
      <p>  <%= @project.name %> </p>
    </div>
  </div>
  <div class="template-example">
    <div class="title">
      <h3>O que vai testar?</h3>
    </div>
    <div class="template-content">
      <p> <%= @project.test_platform %> </p>
    </div>
  </div>

  <div class="template-example">
    <div class="title">
      <h3>URL</h3>
    </div>
    <div class="template-content">
      <p> <%= @project.url %> </p>
    </div>
  </div>
  <%= link_to "Editar Teste", edit_project_path(@project)%>
</div>  

