<%= simple_form_for(@credit, html: {class: "form-signin"}, url: project_credits_path) do |f| %>
  <%= f.error_notification %>


  <%= f.button :submit, :class => "btn btn-lg btn-primary btn-block", value: "Finalizar", 'data-disable-with' => "..." %>
  <%= link_to 'Voltar', :back, :class => "btn btn-primary btn-block btn-lg" %>
<% end %>
